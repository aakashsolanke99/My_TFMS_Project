package com.revature.model;

import java.util.ArrayList;
import java.util.Collection;
import java.util.Collections;
import java.util.Comparator;

public class Appliction {

	public static void main(String[] args) {
		
		Associate a1=new Associate(101,"Aakash","java","B-tech",5);
		Associate a2=new Associate(102,"krunal","dot net","B-tech",3);
		Associate a3=new Associate(103,"Veera","Testing","M-tech",2);
		Associate a4=new Associate(104,"Amruta","mainframe","MCA",5);
		Associate a5=new Associate(105,"arun","mainframe","MCA",12);

		
		ArrayList<Associate> l=new ArrayList();
		l.add(a1);
		l.add(a2);
		l.add(a3);
		l.add(a4);
		l.add(a5);
		System.out.println("before sorting");
		
		for (Associate associate : l) {
			System.out.println(associate);
			
		}
		
		System.out.println("After sorting");
		
		
		Collections.sort( l, new Comparator<Associate>(){
			@Override
			public int compare(Associate o1, Associate o2) {
				// TODO Auto-generated method stub
				return o1.associateExperience-o2.associateExperience;
			}
		
		});
		
		for (Associate associate : l) {
			System.out.println(associate);
			
		}
		

	}

}
