package com.revature.model;

public class Associate {

	    private static int AssociateId;
		private String AssociateName;
		private enum AssociateTrack {
			JAVA,
			DOTNET,
			MAINFRAME,
			TESTING
		}
		private String AssociateQualification;
		private int AssociateExperience;
		
		public static int getAssociateId() {
			return AssociateId;
		}

		public static void setAssociateId(int associateId) {
			AssociateId = associateId;
		}

		public String getAssociateName() {
			return AssociateName;
		}

		public void setAssociateName(String associateName) {
			AssociateName = associateName;
		}

		public String getAssociateQualification() {
			return AssociateQualification;
		}

		public void setAssociateQualification(String associateQualification) {
			AssociateQualification = associateQualification;
		}

		public int getAssociateExperience() {
			return AssociateExperience;
		}

		public void setAssociateExperience(int associateExperience) {
			AssociateExperience = associateExperience;
		}

		public Associate(String associateName, String associateQualification, int associateExperience) {
			super();
			AssociateName = associateName;
			AssociateQualification = associateQualification;
			AssociateExperience = associateExperience;
		}
		
		
		
}
