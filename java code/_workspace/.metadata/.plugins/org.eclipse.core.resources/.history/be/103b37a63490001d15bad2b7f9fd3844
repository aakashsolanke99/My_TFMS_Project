package com.revature.model;

public class Associate {

		protected static int Associate_id;
		protected String Associate_name;
		protected enum Associate_track {
			JAVA,
			DOTNET,
			MAINFRAME,
			TESTING
		}
		protected String Associate_qualification;
		protected int Associate_experience;
		
		public Associate(String associate_name, String associate_qualification, int associate_experience) {
			super();
			Associate_name = associate_name;
			Associate_qualification = associate_qualification;
			Associate_experience = associate_experience;
		}
		public static int getAssociate_id() {
			return Associate_id;
		}
		public static void setAssociate_id(int associate_id) {
			Associate_id = associate_id;
		}
		public String getAssociate_name() {
			return Associate_name;
		}
		public void setAssociate_name(String associate_name) {
			Associate_name = associate_name;
		}
		public String getAssociate_qualification() {
			return Associate_qualification;
		}
		public void setAssociate_qualification(String associate_qualification) {
			Associate_qualification = associate_qualification;
		}
		public int getAssociate_experience() {
			return Associate_experience;
		}
		public void setAssociate_experience(int associate_experience) {
			Associate_experience = associate_experience;
		}
		
}
